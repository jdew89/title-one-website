//- basic navbar fixed to top. Is transparent.
nav.navbar.is-fixed-top(role="navigation" aria-label="main navigation")
	div.navbar-brand
		a.navbar-item(href='/' style="padding-top:0px;padding-bottom:0px;")
			//h1.title LOGO
			img(src="/images/title-logo.png" style="max-height:2.5rem")
		a.navbar-burger.burger(role="button" aria-label="menu" aria-expanded="false" data-target="navbarLinks")
			span(aria-hidden="true")
			span(aria-hidden="true")
			span(aria-hidden="true")
	div#navbarLinks.navbar-menu
		div.navbar-start.is-primary
			a.navbar-item(href="/") Home
			a.navbar-item(href="/services") Our Services
			//-a.navbar-item(href="/") Why us?
			a.navbar-item(href="/") Order Title Services
			a.navbar-item(href="/") Contact Us
			a.navbar-item(href="/") Meet the Team
		//-div.navbar-end
			div.navbar-item
				div.buttons
					a.button.is-primary Sign up
					a.button.is-light Login
